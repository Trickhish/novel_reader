{
    "mangas": {

        "mangabuddy.com": {
            "enabled": true,
            "baseUrl": "https://mangabuddy.com",
            "cloudflare": false,
            "search": {
                "endpoint": "/api/manga/search",
                "type": "GET",
                "payload": {
                    "q": "$query"
                },
                "query_key": "q",
                "extract": {
                    "ops": [
                        ["split", "<div class=\"novel__item\">"],
                        ["subarray", 1]
                    ],
                    "name": [
                        ["cutbtw", "<a title=\"", "\""]
                    ],
                    "link": [
                        ["cutbtw", "href=\"", "\""]
                    ],
                    "thumb": [
                        ["cutbtw", "<img src=\"", "\""]
                    ],
                    "desc": null
                }
            },
            "data": {
                "type": "GET",
                "payload": {},
                "extract": {
                    "name": [
                        ["cutbtw", "<div class=\"name box\">", "</div>"],
                        ["cutbtw", "<h1>", "</h1"]
                    ],
                    "authors": [
                        ["cutbtw", "<p><strong>Authors :</strong>", "</p>"],
                        ["split", "<a"],
                        ["subarray", 1],
                        ["cutbtw", "title=\"", "\""]
                    ],
                    "finished": [
                        ["cutbtw", "<p><strong>Status :</strong>", "</a>"],
                        ["cutbtw", "<span>", "</span>"],
                        ["!=", "Ongoing"]
                    ],
                    "chapters_nb": [
                        ["cutbtw", "<p><strong>Chapters: </strong>", "</p>"],
                        ["cutbtw", "<span>", "</span>"]
                    ],
                    "last_update": [
                        ["cutbtw", "<p><strong>Last update: </strong>", "</p>"],
                        ["cutbtw", "<span>", "</span>"],
                        ["ago_to_ts"]
                    ],
                    "tags": [
                        ["cutbtw", "<p><strong>Genres :</strong>", "</p>"],
                        ["split", "<a"],
                        ["subarray", 1],
                        ["cutbtw", ">", "<"],
                        ["remove_strings", "\n", ","],
                        ["trim"]
                    ],
                    "desc": [
                        ["cutbtw", "<div class=\"section-body summary\">", "<div class=\"readmore\""],
                        ["cutbtw", "<p class=\"content\"", "<div"],
                        ["cutbtw", ">", "<"],
                        ["trim"]
                    ],
                    "rating": [
                        ["cutbtw", "\"ratingValue\":", ","],
                        ["mult", 2]
                    ]
                }
            },
            "id": {
                "type": "GET",
                "payload": {},
                
            }
        },

        "whalemanga.com": {
            "enabled": true,
            "baseUrl": "https://whalemanga.com",
            "cloudflare": false,
            "search": {
                "endpoint": "/wp-admin/admin-ajax.php",
                "type": "POST",
                "payload": {
                    "action": "wp-manga-search-manga",
                    "title": "$query"
                },
                "query_key": "title",
                "extract": {
                    "ops": [
                        ["json_decode"],
                        ["dictSelect", "data"]
                    ],
                    "name": [
                        ["dictSelect", "title"]
                    ],
                    "link": [
                        ["dictSelect", "url"]
                    ],
                    "thumb": null,
                    "desc": null
                }
            }
        },

        "reaperscans.com": {
            "enabled": true,
            "baseUrl": "https://api.reaperscans.com",
            "cloudflare": false,
            "search": {
                "endpoint": "/query",
                "type": "GET",
                "payload": {
                    "adult": "true",
                    "query_string": "$query"
                },
                "query_key": "query_string",
                "extract": {
                    "ops": [
                        ["json_decode"],
                        ["dictSelect", "data"]
                    ],
                    "name": [
                        ["dictSelect", "title"]
                    ],
                    "desc": [
                        ["dictSelect", "description"]
                    ],
                    "thumb": [
                        ["dictSelect", "thumbnail"]
                    ],
                    "link": [
                        ["concat", 
                            "/series/",
                            ["dictSelect", "series_slug"]
                        ]
                    ]
                }
            }
        },

        "natomanga.com": {
            "enabled": false,
            "baseUrl": "https://www.natomanga.com",
            "cloudflare": true,
            "search": {
                "endpoint": "/home/search/json",
                "type": "GET",
                "payload": {
                    "searchword": "$query"
                },
                "query_key": "searchword",
                "extract": {
                    "ops": [
                        "split|<div class=\"novel__item\">",
                        "subarray|1-"
                    ],
                    "name": [
                        "cutbtw|<a title=\"|\""
                    ],
                    "link": [
                        "cutbtw|href=\"|\""
                    ],
                    "thumb": [
                        "cutbtw|<img src=\"|\""
                    ]
                }
            }
        }
    },

    "novels": {

        "webnovelpub.co": {
            "enabled": true,
            "baseUrl": "https://webnovelpub.co",
            "cloudflare": false,
            "search": {
                "endpoint": "/ajax/searchLive",
                "type": "GET",
                "payload": {
                    "inputContent": "$query"
                },
                "query_key": "inputContent",
                "extract": {
                    "ops": [
                        ["split", "class=\\\"novel-item"],
                        ["subarray", 1]
                    ],
                    "name": [
                        ["cutbtw", "title=\\\"", "\\\""]
                    ],
                    "link": [
                        ["cutbtw", "href=\\\"", "\\\""],
                        ["decode_slashes"]
                    ],
                    "thumb": [
                        ["cutbtw", "img src=\\\"", "\\\""],
                        ["decode_slashes"]
                    ]
                }
            }
        },

        "lightnovelworld.com": {
            "enabled": false,
            "baseUrl": "https://www.lightnovelworld.com",
            "cloudflare": false,
            "search": {
                "endpoint": "/lnsearchlive",
                "type": "POST",
                "payload": {
                    "inputContent": "$query"
                },
                "result": {
                    "key": "resultview"
                },
                "query_key": "inputContent"
            }
        },

        "lightnovelworld.co": {
            "enabled": false,
            "baseUrl": "https://www.lightnovelworld.co",
            "cloudflare": false,
            "search": {
                "endpoint": ""
            }
        }
    }
}