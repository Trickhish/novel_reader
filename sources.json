{
    "mangas": {

        "MangaBuddy": {
            "baseUrl": "https://mangabuddy.com",
            "cloudflare": false,
            "search": {
                "endpoint": "/api/manga/search",
                "type": "GET",
                "payload": {
                    "q": "$query"
                },
                "query_key": "q"
            }
        },

        "NatoManga": {
            "baseUrl": "https://www.natomanga.com",
            "cloudflare": true,
            "search": {
                "endpoint": "/home/search/json",
                "type": "GET",
                "payload": {
                    "searchword": "$query"
                },
                "query_key": "searchword",
                "extract": {
                    
                }
            }
        }
    },

    "novels": {

        "lightnovelworld.com": {
            "baseUrl": "https://www.lightnovelworld.com",
            "cloudflare": false,
            "search": {
                "endpoint": "/lnsearchlive",
                "type": "POST",
                "payload": {
                    "inputContent": "$query"
                },
                "result": {
                    "key": "resultview"
                },
                "query_key": "inputContent"
            }
        },

        "lightnovelworld.co": {
            "baseUrl": "https://www.lightnovelworld.co",
            "cloudflare": false,
            "search": {
                "endpoint": ""
            }
        }
    }
}