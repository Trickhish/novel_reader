{
    "mangas": {

        "MangaBuddy": {
            "enabled": true,
            "baseUrl": "https://mangabuddy.com",
            "cloudflare": false,
            "search": {
                "endpoint": "/api/manga/search",
                "type": "GET",
                "payload": {
                    "q": "$query"
                },
                "query_key": "q",
                "extract": {
                    "ops": [
                        ["split", "<div class=\"novel__item\">"],
                        ["subarray", 1]
                    ],
                    "name": [
                        ["cutbtw", "<a title=\"", "\""]
                    ],
                    "link": [
                        ["cutbtw", "href=\"", "\""]
                    ],
                    "thumb": [
                        ["cutbtw", "<img src=\"", "\""]
                    ]
                }
            }
        },

        "NatoManga": {
            "enabled": false,
            "baseUrl": "https://www.natomanga.com",
            "cloudflare": true,
            "search": {
                "endpoint": "/home/search/json",
                "type": "GET",
                "payload": {
                    "searchword": "$query"
                },
                "query_key": "searchword",
                "extract": {
                    "ops": [
                        "split|<div class=\"novel__item\">",
                        "subarray|1-"
                    ],
                    "name": [
                        "cutbtw|<a title=\"|\""
                    ],
                    "link": [
                        "cutbtw|href=\"|\""
                    ],
                    "thumb": [
                        "cutbtw|<img src=\"|\""
                    ]
                }
            }
        }
    },

    "novels": {

        "lightnovelworld.com": {
            "baseUrl": "https://www.lightnovelworld.com",
            "cloudflare": false,
            "search": {
                "endpoint": "/lnsearchlive",
                "type": "POST",
                "payload": {
                    "inputContent": "$query"
                },
                "result": {
                    "key": "resultview"
                },
                "query_key": "inputContent"
            }
        },

        "lightnovelworld.co": {
            "baseUrl": "https://www.lightnovelworld.co",
            "cloudflare": false,
            "search": {
                "endpoint": ""
            }
        }
    }
}